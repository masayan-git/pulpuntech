<p id="notice"><%= notice %></p>

<h1>Masters Articles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @masters_articles.each do |masters_article| %>
      
        <p>
          <%= masters_article.title %>
          <%= link_to 'Show', [:masters, masters_article] %></td>
          <%= link_to 'Edit', edit_masters_article_path(masters_article) %></td>
          <%= link_to 'Destroy', [:masters, masters_article], method: :delete, data: { confirm: '削除しますか？' } %>
        </p>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Masters Article', new_masters_article_path %>
